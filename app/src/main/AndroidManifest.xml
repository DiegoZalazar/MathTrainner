<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".principalAmplify"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MathTrainer"
        tools:targetApi="31">
        <activity
            android:name=".AdminUI.CommonComposables"
            android:exported="false"
            android:label="@string/title_activity_common_composables"
            android:theme="@style/Theme.MathTrainer" />
        <activity
            android:name=".AdminUI.AdminEditUserActivity"
            android:exported="false"
            android:label="@string/title_activity_admin_edit_user"
            android:theme="@style/Theme.MathTrainer" />
        <activity
            android:name=".AdminUI.AdminDeleteUserActivity"
            android:exported="false"
            android:label="@string/title_activity_admin_delete_user"
            android:theme="@style/Theme.MathTrainer" />
        <activity
            android:name=".AdminEjerciciosActivity"
            android:exported="false"
            android:label="@string/title_activity_admin_ejercicios"
            android:theme="@style/Theme.MathTrainer" />
        <activity
            android:name=".AdminLeccionesActivity"
            android:exported="false"
            android:label="@string/title_activity_admin_lecciones"
            android:theme="@style/Theme.MathTrainer" />
        <activity
            android:name=".AdminUI.AdminEditModActivity"
            android:exported="false"
            android:label="@string/title_activity_admin_edit_mod"
            android:theme="@style/Theme.MathTrainer" />
        <activity
            android:name=".AdminUI.AdminModulosActivity"
            android:exported="false"
            android:label="@string/title_activity_admin_modulos"
            android:theme="@style/Theme.MathTrainer" />
        <activity
            android:name=".AdminUI.AdminUsuariosActivity"
            android:exported="false"
            android:label="@string/title_activity_admin_usuarios"
            android:theme="@style/Theme.MathTrainer" />
        <activity
            android:name=".AdminUI.AdminPrincipalActivity"
            android:exported="true"
            android:label="@string/title_activity_admin_principal"
            android:theme="@style/Theme.MathTrainer" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.MathTrainer">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>